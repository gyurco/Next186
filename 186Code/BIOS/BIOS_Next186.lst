				;--------------------------------------------------------------------------
				; Feature byte 1
				; b7: 1=DMA channel 3 used by hard disk
				; b6: 1=2 interrupt controllers present
				; b5: 1=RTC present
				; b4: 1=BIOS calls int 15h/4Fh every key
				; b3: 1=wait for extern event supported (Int 15h/41h)
				; b2: 1=extended BIOS data area used
				; b1: 0=AT or ESDI bus, 1=MicroChannel
				; b0: 1=Dual bus (MicroChannel + ISA)
				;--------------------------------------------------------------------------
 F340  10						db      10h
				;--------------------------------------------------------------------------
				; Feature byte 2
				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F341  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F342  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F343  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F344  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F345				int16 proc near
 F345  1E					push    ds
 F346  56					push    si
 F347  6A 40					push    40h
 F349  1F					pop     ds
 F34A  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F34C  48					dec     ax
 F34D  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F34F  74 3E					jz      short TestKey   ;TestKey does not need cld
 F351  40					inc     ax
 F352  FC					cld
 F353  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F355  74 1C					jz      short GetKey
 F357  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F359  72 78					jb      short GetStatus
 F35B  74 7B					je      short SetAutoRpt   
 F35D  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F35F  74 3D					je      short StoreKey
 F361  3C 09					cmp     al, 9           ;Get KB functionality
 F363  74 08					je      short kbfunc     
 F365  3C 12					cmp     al, 12h         ;Extended status call
 F367  74 56					je      short ExtStatus
 F369  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F36B  75 02					jne     short Exit
 F36D				kbfunc:
 F36D  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F36F				Exit:        
 F36F  5E					pop     si
 F370  1F					pop     ds
 F371  CF					iret                    ; unknown function, Restores flags.

 F372				GetKey1:                        ; wait for interrupt
 F372  F4					hlt
 F373				GetKey: ; ----------- fn 00h, 10h
 F373  B4 11					mov     ah, 11h
 F375  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F377  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F379  FA					cli                     ;Critical region! Ints off.
 F37A  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F37E  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F37F  3B 36 0082				cmp     si, EndBuf
 F383  72 04					jb      short noWrap
 F385  8B 36 0080				mov     si, Buffer
 F389				noWrap:             
 F389  89 36 001A				mov     HeadPtr, si
 F38D  EB E0					jmp     short Exit

 F38F				TestKey: ; ---------- fn 01h
 F38F  8B 36 001A				mov     si, HeadPtr
 F393  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F397  AD					lodsw                   ;BIOS returns avail keycode.
 F398  FB					sti                     ;Ints back on.
 F399  5E					pop     si
 F39A  1F					pop     ds
 F39B  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F39E				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F39E  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F3A2  89 0C					mov     [si], cx        ;Store the key code away
 F3A4  46					inc     si
 F3A5  46					inc     si              ;Move on to next entry in buf
 F3A6  3B 36 0082				cmp     si, EndBuf
 F3AA  72 04					jb      short NoWrap1
 F3AC  8B 36 0080				mov     si, Buffer
 F3B0				 NoWrap1:
 F3B0  B0 01					mov     al, 1           ;no room
 F3B2  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3B6  74 B7					je      short Exit      ;if so, ignore key entry.
 F3B8  89 36 001C				mov     TailPtr, si
 F3BC  48					dec     ax              ;al=0
 F3BD  EB B0					jmp     short Exit       

 F3BF				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3BF  A0 0018					mov     al, KbdFlags2
 F3C2  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3C4  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3C6  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3C8  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3CA				NoSysReq:
 F3CA  8A 26 0096				mov     ah, KbdFlags3
 F3CE  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3D1  0A E0					or      ah, al          ;Merge into AH.

 F3D3				GetStatus: ; --------- fn 02h     
 F3D3  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3D6				Exit1:
 F3D6  EB 97					jmp     short Exit

 F3D8				SetAutoRpt: ; ------ fn 03h
 F3D8  80 FC 05					cmp     ah, 5
 F3DB  75 92					jne     short Exit
 F3DD  52					push    dx
 F3DE  C0 E7 05					shl     bh, 5
 F3E1  80 E3 1F					and     bl, 1fh
 F3E4  0A DF					or      bl, bh
 F3E6  80 E3 7F					and     bl, 7fh
 F3E9  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3EB  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3EE  72 23					jc      short timeout
 F3F0  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F3F5  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F3F7  53					push    bx
 F3F8  32 DB					xor     bl, bl          ; send to kb
 F3FA  E8 01AB					call    sendps2byte
 F3FD  5B					pop     bx
 F3FE  72 0E					jc      short timeout1  ; send timeout
 F400  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F402  E8 0011					call    WaitFlag
 F405  72 07					jc      short timeout1
 F407  8A E3					mov     ah, bl
 F409  32 DB					xor     bl, bl          ; send to kb
 F40B  E8 019A					call    sendps2byte     ; send data
 F40E				timeout1:
 F40E  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F413				timeout:
 F413  5A					pop     dx
 F414  EB C0					jmp     short Exit1


 F416				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F416  BA 03DA					mov     dx, 3dah
 F419  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F41B				wf_loop:
 F41B  FA					cli
 F41C  A0 0097					mov     al, KbdFlags4
 F41F  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F421  38 E0					cmp     al, ah
 F423  74 0A					je      short wf_ok ; flag ok, CF=0
 F425  FB					sti
 F426  EC					in      al, dx      ; get vblank
 F427  32 C7					xor     al, bh
 F429  24 08					and     al, 8h
 F42B  2A F8					sub     bh, al
 F42D  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F42F				wf_ok:
 F42F  C3					ret
 F430				int16 endp

