				; b4: 1=BIOS calls int 15h/4Fh every key
				; b3: 1=wait for extern event supported (Int 15h/41h)
				; b2: 1=extended BIOS data area used
				; b1: 0=AT or ESDI bus, 1=MicroChannel
				; b0: 1=Dual bus (MicroChannel + ISA)
				;--------------------------------------------------------------------------
 F34F  10						db      10h
				;--------------------------------------------------------------------------
				; Feature byte 2
				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F350  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F351  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F352  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F353  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F354				int16 proc near
 F354  1E					push    ds
 F355  56					push    si
 F356  6A 40					push    40h
 F358  1F					pop     ds
 F359  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F35B  48					dec     ax
 F35C  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F35E  74 3E					jz      short TestKey   ;TestKey does not need cld
 F360  40					inc     ax
 F361  FC					cld
 F362  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F364  74 1C					jz      short GetKey
 F366  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F368  72 78					jb      short GetStatus
 F36A  74 7B					je      short SetAutoRpt   
 F36C  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F36E  74 3D					je      short StoreKey
 F370  3C 09					cmp     al, 9           ;Get KB functionality
 F372  74 08					je      short kbfunc     
 F374  3C 12					cmp     al, 12h         ;Extended status call
 F376  74 56					je      short ExtStatus
 F378  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F37A  75 02					jne     short Exit
 F37C				kbfunc:
 F37C  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F37E				Exit:        
 F37E  5E					pop     si
 F37F  1F					pop     ds
 F380  CF					iret                    ; unknown function, Restores flags.

 F381				GetKey1:                        ; wait for interrupt
 F381  F4					hlt
 F382				GetKey: ; ----------- fn 00h, 10h
 F382  B4 11					mov     ah, 11h
 F384  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F386  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F388  FA					cli                     ;Critical region! Ints off.
 F389  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F38D  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F38E  3B 36 0082				cmp     si, EndBuf
 F392  72 04					jb      short noWrap
 F394  8B 36 0080				mov     si, Buffer
 F398				noWrap:             
 F398  89 36 001A				mov     HeadPtr, si
 F39C  EB E0					jmp     short Exit

 F39E				TestKey: ; ---------- fn 01h
 F39E  8B 36 001A				mov     si, HeadPtr
 F3A2  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F3A6  AD					lodsw                   ;BIOS returns avail keycode.
 F3A7  FB					sti                     ;Ints back on.
 F3A8  5E					pop     si
 F3A9  1F					pop     ds
 F3AA  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F3AD				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F3AD  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F3B1  89 0C					mov     [si], cx        ;Store the key code away
 F3B3  46					inc     si
 F3B4  46					inc     si              ;Move on to next entry in buf
 F3B5  3B 36 0082				cmp     si, EndBuf
 F3B9  72 04					jb      short NoWrap1
 F3BB  8B 36 0080				mov     si, Buffer
 F3BF				 NoWrap1:
 F3BF  B0 01					mov     al, 1           ;no room
 F3C1  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3C5  74 B7					je      short Exit      ;if so, ignore key entry.
 F3C7  89 36 001C				mov     TailPtr, si
 F3CB  48					dec     ax              ;al=0
 F3CC  EB B0					jmp     short Exit       

 F3CE				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3CE  A0 0018					mov     al, KbdFlags2
 F3D1  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3D3  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3D5  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3D7  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3D9				NoSysReq:
 F3D9  8A 26 0096				mov     ah, KbdFlags3
 F3DD  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3E0  0A E0					or      ah, al          ;Merge into AH.

 F3E2				GetStatus: ; --------- fn 02h     
 F3E2  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3E5				Exit1:
 F3E5  EB 97					jmp     short Exit

 F3E7				SetAutoRpt: ; ------ fn 03h
 F3E7  80 FC 05					cmp     ah, 5
 F3EA  75 92					jne     short Exit
 F3EC  52					push    dx
 F3ED  C0 E7 05					shl     bh, 5
 F3F0  80 E3 1F					and     bl, 1fh
 F3F3  0A DF					or      bl, bh
 F3F5  80 E3 7F					and     bl, 7fh
 F3F8  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3FA  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3FD  72 23					jc      short timeout
 F3FF  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F404  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F406  53					push    bx
 F407  32 DB					xor     bl, bl          ; send to kb
 F409  E8 01AB					call    sendps2byte
 F40C  5B					pop     bx
 F40D  72 0E					jc      short timeout1  ; send timeout
 F40F  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F411  E8 0011					call    WaitFlag
 F414  72 07					jc      short timeout1
 F416  8A E3					mov     ah, bl
 F418  32 DB					xor     bl, bl          ; send to kb
 F41A  E8 019A					call    sendps2byte     ; send data
 F41D				timeout1:
 F41D  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F422				timeout:
 F422  5A					pop     dx
 F423  EB C0					jmp     short Exit1


 F425				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F425  BA 03DA					mov     dx, 3dah
 F428  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F42A				wf_loop:
 F42A  FA					cli
 F42B  A0 0097					mov     al, KbdFlags4
 F42E  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F430  38 E0					cmp     al, ah
 F432  74 0A					je      short wf_ok ; flag ok, CF=0
 F434  FB					sti
 F435  EC					in      al, dx      ; get vblank
 F436  32 C7					xor     al, bh
 F438  24 08					and     al, 8h
 F43A  2A F8					sub     bh, al
 F43C  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F43E				wf_ok:
 F43E  C3					ret
 F43F				int16 endp

