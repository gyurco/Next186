				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F30F  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F310				int16 proc near
 F310  1E					push    ds
 F311  56					push    si
 F312  6A 40					push    40h
 F314  1F					pop     ds
 F315  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F317  48					dec     ax
 F318  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F31A  74 3E					jz      short TestKey   ;TestKey does not need cld
 F31C  40					inc     ax
 F31D  FC					cld
 F31E  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F320  74 1C					jz      short GetKey
 F322  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F324  72 78					jb      short GetStatus
 F326  74 7B					je      short SetAutoRpt   
 F328  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F32A  74 3D					je      short StoreKey
 F32C  3C 09					cmp     al, 9           ;Get KB functionality
 F32E  74 08					je      short kbfunc     
 F330  3C 12					cmp     al, 12h         ;Extended status call
 F332  74 56					je      short ExtStatus
 F334  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F336  75 02					jne     short Exit
 F338				kbfunc:
 F338  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F33A				Exit:        
 F33A  5E					pop     si
 F33B  1F					pop     ds
 F33C  CF					iret                    ; unknown function, Restores flags.

 F33D				GetKey1:                        ; wait for interrupt
 F33D  F4					hlt
 F33E				GetKey: ; ----------- fn 00h, 10h
 F33E  B4 11					mov     ah, 11h
 F340  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F342  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F344  FA					cli                     ;Critical region! Ints off.
 F345  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F349  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F34A  3B 36 0082				cmp     si, EndBuf
 F34E  72 04					jb      short noWrap
 F350  8B 36 0080				mov     si, Buffer
 F354				noWrap:             
 F354  89 36 001A				mov     HeadPtr, si
 F358  EB E0					jmp     short Exit

 F35A				TestKey: ; ---------- fn 01h
 F35A  8B 36 001A				mov     si, HeadPtr
 F35E  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F362  AD					lodsw                   ;BIOS returns avail keycode.
 F363  FB					sti                     ;Ints back on.
 F364  5E					pop     si
 F365  1F					pop     ds
 F366  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F369				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F369  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F36D  89 0C					mov     [si], cx        ;Store the key code away
 F36F  46					inc     si
 F370  46					inc     si              ;Move on to next entry in buf
 F371  3B 36 0082				cmp     si, EndBuf
 F375  72 04					jb      short NoWrap1
 F377  8B 36 0080				mov     si, Buffer
 F37B				 NoWrap1:
 F37B  B0 01					mov     al, 1           ;no room
 F37D  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F381  74 B7					je      short Exit      ;if so, ignore key entry.
 F383  89 36 001C				mov     TailPtr, si
 F387  48					dec     ax              ;al=0
 F388  EB B0					jmp     short Exit       

 F38A				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F38A  A0 0018					mov     al, KbdFlags2
 F38D  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F38F  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F391  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F393  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F395				NoSysReq:
 F395  8A 26 0096				mov     ah, KbdFlags3
 F399  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F39C  0A E0					or      ah, al          ;Merge into AH.

 F39E				GetStatus: ; --------- fn 02h     
 F39E  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3A1				Exit1:
 F3A1  EB 97					jmp     short Exit

 F3A3				SetAutoRpt: ; ------ fn 03h
 F3A3  80 FC 05					cmp     ah, 5
 F3A6  75 92					jne     short Exit
 F3A8  52					push    dx
 F3A9  C0 E7 05					shl     bh, 5
 F3AC  80 E3 1F					and     bl, 1fh
 F3AF  0A DF					or      bl, bh
 F3B1  80 E3 7F					and     bl, 7fh
 F3B4  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3B6  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3B9  72 23					jc      short timeout
 F3BB  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F3C0  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F3C2  53					push    bx
 F3C3  32 DB					xor     bl, bl          ; send to kb
 F3C5  E8 0199					call    sendps2byte
 F3C8  5B					pop     bx
 F3C9  72 0E					jc      short timeout1  ; send timeout
 F3CB  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F3CD  E8 0011					call    WaitFlag
 F3D0  72 07					jc      short timeout1
 F3D2  8A E3					mov     ah, bl
 F3D4  32 DB					xor     bl, bl          ; send to kb
 F3D6  E8 0188					call    sendps2byte     ; send data
 F3D9				timeout1:
 F3D9  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F3DE				timeout:
 F3DE  5A					pop     dx
 F3DF  EB C0					jmp     short Exit1


 F3E1				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F3E1  BA 03DA					mov     dx, 3dah
 F3E4  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F3E6				wf_loop:
 F3E6  FA					cli
 F3E7  A0 0097					mov     al, KbdFlags4
 F3EA  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F3EC  38 E0					cmp     al, ah
 F3EE  74 0A					je      short wf_ok ; flag ok, CF=0
 F3F0  FB					sti
 F3F1  EC					in      al, dx      ; get vblank
 F3F2  32 C7					xor     al, bh
 F3F4  24 08					and     al, 8h
 F3F6  2A F8					sub     bh, al
 F3F8  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F3FA				wf_ok:
 F3FA  C3					ret
 F3FB				int16 endp

