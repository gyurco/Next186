				; b5: 1=RTC present
				; b4: 1=BIOS calls int 15h/4Fh every key
				; b3: 1=wait for extern event supported (Int 15h/41h)
				; b2: 1=extended BIOS data area used
				; b1: 0=AT or ESDI bus, 1=MicroChannel
				; b0: 1=Dual bus (MicroChannel + ISA)
				;--------------------------------------------------------------------------
 F337  10						db      10h
				;--------------------------------------------------------------------------
				; Feature byte 2
				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F338  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F339  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F33A  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F33B  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F33C				int16 proc near
 F33C  1E					push    ds
 F33D  56					push    si
 F33E  6A 40					push    40h
 F340  1F					pop     ds
 F341  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F343  48					dec     ax
 F344  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F346  74 3E					jz      short TestKey   ;TestKey does not need cld
 F348  40					inc     ax
 F349  FC					cld
 F34A  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F34C  74 1C					jz      short GetKey
 F34E  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F350  72 78					jb      short GetStatus
 F352  74 7B					je      short SetAutoRpt   
 F354  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F356  74 3D					je      short StoreKey
 F358  3C 09					cmp     al, 9           ;Get KB functionality
 F35A  74 08					je      short kbfunc     
 F35C  3C 12					cmp     al, 12h         ;Extended status call
 F35E  74 56					je      short ExtStatus
 F360  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F362  75 02					jne     short Exit
 F364				kbfunc:
 F364  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F366				Exit:        
 F366  5E					pop     si
 F367  1F					pop     ds
 F368  CF					iret                    ; unknown function, Restores flags.

 F369				GetKey1:                        ; wait for interrupt
 F369  F4					hlt
 F36A				GetKey: ; ----------- fn 00h, 10h
 F36A  B4 11					mov     ah, 11h
 F36C  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F36E  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F370  FA					cli                     ;Critical region! Ints off.
 F371  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F375  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F376  3B 36 0082				cmp     si, EndBuf
 F37A  72 04					jb      short noWrap
 F37C  8B 36 0080				mov     si, Buffer
 F380				noWrap:             
 F380  89 36 001A				mov     HeadPtr, si
 F384  EB E0					jmp     short Exit

 F386				TestKey: ; ---------- fn 01h
 F386  8B 36 001A				mov     si, HeadPtr
 F38A  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F38E  AD					lodsw                   ;BIOS returns avail keycode.
 F38F  FB					sti                     ;Ints back on.
 F390  5E					pop     si
 F391  1F					pop     ds
 F392  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F395				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F395  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F399  89 0C					mov     [si], cx        ;Store the key code away
 F39B  46					inc     si
 F39C  46					inc     si              ;Move on to next entry in buf
 F39D  3B 36 0082				cmp     si, EndBuf
 F3A1  72 04					jb      short NoWrap1
 F3A3  8B 36 0080				mov     si, Buffer
 F3A7				 NoWrap1:
 F3A7  B0 01					mov     al, 1           ;no room
 F3A9  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3AD  74 B7					je      short Exit      ;if so, ignore key entry.
 F3AF  89 36 001C				mov     TailPtr, si
 F3B3  48					dec     ax              ;al=0
 F3B4  EB B0					jmp     short Exit       

 F3B6				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3B6  A0 0018					mov     al, KbdFlags2
 F3B9  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3BB  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3BD  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3BF  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3C1				NoSysReq:
 F3C1  8A 26 0096				mov     ah, KbdFlags3
 F3C5  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3C8  0A E0					or      ah, al          ;Merge into AH.

 F3CA				GetStatus: ; --------- fn 02h     
 F3CA  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3CD				Exit1:
 F3CD  EB 97					jmp     short Exit

 F3CF				SetAutoRpt: ; ------ fn 03h
 F3CF  80 FC 05					cmp     ah, 5
 F3D2  75 92					jne     short Exit
 F3D4  52					push    dx
 F3D5  C0 E7 05					shl     bh, 5
 F3D8  80 E3 1F					and     bl, 1fh
 F3DB  0A DF					or      bl, bh
 F3DD  80 E3 7F					and     bl, 7fh
 F3E0  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3E2  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3E5  72 23					jc      short timeout
 F3E7  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F3EC  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F3EE  53					push    bx
 F3EF  32 DB					xor     bl, bl          ; send to kb
 F3F1  E8 01AB					call    sendps2byte
 F3F4  5B					pop     bx
 F3F5  72 0E					jc      short timeout1  ; send timeout
 F3F7  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F3F9  E8 0011					call    WaitFlag
 F3FC  72 07					jc      short timeout1
 F3FE  8A E3					mov     ah, bl
 F400  32 DB					xor     bl, bl          ; send to kb
 F402  E8 019A					call    sendps2byte     ; send data
 F405				timeout1:
 F405  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F40A				timeout:
 F40A  5A					pop     dx
 F40B  EB C0					jmp     short Exit1


 F40D				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F40D  BA 03DA					mov     dx, 3dah
 F410  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F412				wf_loop:
 F412  FA					cli
 F413  A0 0097					mov     al, KbdFlags4
 F416  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F418  38 E0					cmp     al, ah
 F41A  74 0A					je      short wf_ok ; flag ok, CF=0
 F41C  FB					sti
 F41D  EC					in      al, dx      ; get vblank
 F41E  32 C7					xor     al, bh
 F420  24 08					and     al, 8h
 F422  2A F8					sub     bh, al
 F424  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F426				wf_ok:
 F426  C3					ret
 F427				int16 endp

