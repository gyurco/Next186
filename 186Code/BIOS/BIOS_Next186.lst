				; b4: 1=BIOS calls int 15h/4Fh every key
				; b3: 1=wait for extern event supported (Int 15h/41h)
				; b2: 1=extended BIOS data area used
				; b1: 0=AT or ESDI bus, 1=MicroChannel
				; b0: 1=Dual bus (MicroChannel + ISA)
				;--------------------------------------------------------------------------
 F348  10						db      10h
				;--------------------------------------------------------------------------
				; Feature byte 2
				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F349  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F34A  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F34B  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F34C  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F34D				int16 proc near
 F34D  1E					push    ds
 F34E  56					push    si
 F34F  6A 40					push    40h
 F351  1F					pop     ds
 F352  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F354  48					dec     ax
 F355  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F357  74 3E					jz      short TestKey   ;TestKey does not need cld
 F359  40					inc     ax
 F35A  FC					cld
 F35B  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F35D  74 1C					jz      short GetKey
 F35F  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F361  72 78					jb      short GetStatus
 F363  74 7B					je      short SetAutoRpt   
 F365  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F367  74 3D					je      short StoreKey
 F369  3C 09					cmp     al, 9           ;Get KB functionality
 F36B  74 08					je      short kbfunc     
 F36D  3C 12					cmp     al, 12h         ;Extended status call
 F36F  74 56					je      short ExtStatus
 F371  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F373  75 02					jne     short Exit
 F375				kbfunc:
 F375  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F377				Exit:        
 F377  5E					pop     si
 F378  1F					pop     ds
 F379  CF					iret                    ; unknown function, Restores flags.

 F37A				GetKey1:                        ; wait for interrupt
 F37A  F4					hlt
 F37B				GetKey: ; ----------- fn 00h, 10h
 F37B  B4 11					mov     ah, 11h
 F37D  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F37F  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F381  FA					cli                     ;Critical region! Ints off.
 F382  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F386  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F387  3B 36 0082				cmp     si, EndBuf
 F38B  72 04					jb      short noWrap
 F38D  8B 36 0080				mov     si, Buffer
 F391				noWrap:             
 F391  89 36 001A				mov     HeadPtr, si
 F395  EB E0					jmp     short Exit

 F397				TestKey: ; ---------- fn 01h
 F397  8B 36 001A				mov     si, HeadPtr
 F39B  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F39F  AD					lodsw                   ;BIOS returns avail keycode.
 F3A0  FB					sti                     ;Ints back on.
 F3A1  5E					pop     si
 F3A2  1F					pop     ds
 F3A3  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F3A6				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F3A6  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F3AA  89 0C					mov     [si], cx        ;Store the key code away
 F3AC  46					inc     si
 F3AD  46					inc     si              ;Move on to next entry in buf
 F3AE  3B 36 0082				cmp     si, EndBuf
 F3B2  72 04					jb      short NoWrap1
 F3B4  8B 36 0080				mov     si, Buffer
 F3B8				 NoWrap1:
 F3B8  B0 01					mov     al, 1           ;no room
 F3BA  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3BE  74 B7					je      short Exit      ;if so, ignore key entry.
 F3C0  89 36 001C				mov     TailPtr, si
 F3C4  48					dec     ax              ;al=0
 F3C5  EB B0					jmp     short Exit       

 F3C7				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3C7  A0 0018					mov     al, KbdFlags2
 F3CA  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3CC  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3CE  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3D0  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3D2				NoSysReq:
 F3D2  8A 26 0096				mov     ah, KbdFlags3
 F3D6  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3D9  0A E0					or      ah, al          ;Merge into AH.

 F3DB				GetStatus: ; --------- fn 02h     
 F3DB  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3DE				Exit1:
 F3DE  EB 97					jmp     short Exit

 F3E0				SetAutoRpt: ; ------ fn 03h
 F3E0  80 FC 05					cmp     ah, 5
 F3E3  75 92					jne     short Exit
 F3E5  52					push    dx
 F3E6  C0 E7 05					shl     bh, 5
 F3E9  80 E3 1F					and     bl, 1fh
 F3EC  0A DF					or      bl, bh
 F3EE  80 E3 7F					and     bl, 7fh
 F3F1  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3F3  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3F6  72 23					jc      short timeout
 F3F8  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F3FD  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F3FF  53					push    bx
 F400  32 DB					xor     bl, bl          ; send to kb
 F402  E8 01AB					call    sendps2byte
 F405  5B					pop     bx
 F406  72 0E					jc      short timeout1  ; send timeout
 F408  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F40A  E8 0011					call    WaitFlag
 F40D  72 07					jc      short timeout1
 F40F  8A E3					mov     ah, bl
 F411  32 DB					xor     bl, bl          ; send to kb
 F413  E8 019A					call    sendps2byte     ; send data
 F416				timeout1:
 F416  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F41B				timeout:
