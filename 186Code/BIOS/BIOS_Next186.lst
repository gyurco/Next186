				; b3: 1=wait for extern event supported (Int 15h/41h)
				; b2: 1=extended BIOS data area used
				; b1: 0=AT or ESDI bus, 1=MicroChannel
				; b0: 1=Dual bus (MicroChannel + ISA)
				;--------------------------------------------------------------------------
 F34D  10						db      10h
				;--------------------------------------------------------------------------
				; Feature byte 2
				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F34E  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F34F  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F350  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F351  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F352				int16 proc near
 F352  1E					push    ds
 F353  56					push    si
 F354  6A 40					push    40h
 F356  1F					pop     ds
 F357  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F359  48					dec     ax
 F35A  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F35C  74 3E					jz      short TestKey   ;TestKey does not need cld
 F35E  40					inc     ax
 F35F  FC					cld
 F360  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F362  74 1C					jz      short GetKey
 F364  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F366  72 78					jb      short GetStatus
 F368  74 7B					je      short SetAutoRpt   
 F36A  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F36C  74 3D					je      short StoreKey
 F36E  3C 09					cmp     al, 9           ;Get KB functionality
 F370  74 08					je      short kbfunc     
 F372  3C 12					cmp     al, 12h         ;Extended status call
 F374  74 56					je      short ExtStatus
 F376  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F378  75 02					jne     short Exit
 F37A				kbfunc:
 F37A  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F37C				Exit:        
 F37C  5E					pop     si
 F37D  1F					pop     ds
 F37E  CF					iret                    ; unknown function, Restores flags.

 F37F				GetKey1:                        ; wait for interrupt
 F37F  F4					hlt
 F380				GetKey: ; ----------- fn 00h, 10h
 F380  B4 11					mov     ah, 11h
 F382  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F384  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F386  FA					cli                     ;Critical region! Ints off.
 F387  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F38B  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F38C  3B 36 0082				cmp     si, EndBuf
 F390  72 04					jb      short noWrap
 F392  8B 36 0080				mov     si, Buffer
 F396				noWrap:             
 F396  89 36 001A				mov     HeadPtr, si
 F39A  EB E0					jmp     short Exit

 F39C				TestKey: ; ---------- fn 01h
 F39C  8B 36 001A				mov     si, HeadPtr
 F3A0  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F3A4  AD					lodsw                   ;BIOS returns avail keycode.
 F3A5  FB					sti                     ;Ints back on.
 F3A6  5E					pop     si
 F3A7  1F					pop     ds
 F3A8  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F3AB				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F3AB  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F3AF  89 0C					mov     [si], cx        ;Store the key code away
 F3B1  46					inc     si
 F3B2  46					inc     si              ;Move on to next entry in buf
 F3B3  3B 36 0082				cmp     si, EndBuf
 F3B7  72 04					jb      short NoWrap1
 F3B9  8B 36 0080				mov     si, Buffer
 F3BD				 NoWrap1:
 F3BD  B0 01					mov     al, 1           ;no room
 F3BF  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3C3  74 B7					je      short Exit      ;if so, ignore key entry.
 F3C5  89 36 001C				mov     TailPtr, si
 F3C9  48					dec     ax              ;al=0
 F3CA  EB B0					jmp     short Exit       

 F3CC				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3CC  A0 0018					mov     al, KbdFlags2
 F3CF  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3D1  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3D3  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3D5  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3D7				NoSysReq:
 F3D7  8A 26 0096				mov     ah, KbdFlags3
 F3DB  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3DE  0A E0					or      ah, al          ;Merge into AH.

 F3E0				GetStatus: ; --------- fn 02h     
 F3E0  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3E3				Exit1:
 F3E3  EB 97					jmp     short Exit

 F3E5				SetAutoRpt: ; ------ fn 03h
 F3E5  80 FC 05					cmp     ah, 5
 F3E8  75 92					jne     short Exit
 F3EA  52					push    dx
 F3EB  C0 E7 05					shl     bh, 5
 F3EE  80 E3 1F					and     bl, 1fh
 F3F1  0A DF					or      bl, bh
 F3F3  80 E3 7F					and     bl, 7fh
 F3F6  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3F8  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3FB  72 23					jc      short timeout
 F3FD  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F402  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F404  53					push    bx
 F405  32 DB					xor     bl, bl          ; send to kb
 F407  E8 01AB					call    sendps2byte
 F40A  5B					pop     bx
 F40B  72 0E					jc      short timeout1  ; send timeout
 F40D  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F40F  E8 0011					call    WaitFlag
 F412  72 07					jc      short timeout1
 F414  8A E3					mov     ah, bl
 F416  32 DB					xor     bl, bl          ; send to kb
 F418  E8 019A					call    sendps2byte     ; send data
 F41B				timeout1:
 F41B  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F420				timeout:
 F420  5A					pop     dx
 F421  EB C0					jmp     short Exit1


 F423				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F423  BA 03DA					mov     dx, 3dah
 F426  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F428				wf_loop:
 F428  FA					cli
 F429  A0 0097					mov     al, KbdFlags4
 F42C  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F42E  38 E0					cmp     al, ah
 F430  74 0A					je      short wf_ok ; flag ok, CF=0
 F432  FB					sti
 F433  EC					in      al, dx      ; get vblank
 F434  32 C7					xor     al, bh
 F436  24 08					and     al, 8h
 F438  2A F8					sub     bh, al
 F43A  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F43C				wf_ok:
 F43C  C3					ret
 F43D				int16 endp

