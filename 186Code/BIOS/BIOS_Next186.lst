 F569  A8 02					test    al, 2
 F56B  74 0B					jz      short sps2b1; buffer empty
 F56D  EC					in      al, dx      ; get vblank
 F56E  32 C7					xor     al, bh
 F570  24 08					and     al, 8h
 F572  2A F8					sub     bh, al
