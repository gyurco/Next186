				; b7: 1=32-bit DMA supported
				; b6: 1=int16h, function 9 supported
				; b5: 1=int15h/C6h (get POS data) supported
				; b4: 1=int15h/C7h (get mem map info) supported
				; b3: 1=int15h/C8h (en/dis CPU) supported
				; b2: 1=non-8042 kb controller
				; b1: 1=data streaming supported
				; b0: reserved
				;--------------------------------------------------------------------------
 F340  44						db      44h
				;--------------------------------------------------------------------------
				; Feature byte 3
				; b7: not used
				; b6: reserved
				; b5: reserved
				; b4: POST supports ROM-to-RAM enable/disable
				; b3: SCSI on system board
				; b2: info panel installed
				; b1: Initial Machine Load (IML) system - BIOS on disk
				; b0: SCSI supported in IML
				;--------------------------------------------------------------------------
 F341  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 4
				; b7: IBM private
				; b6: EEPROM present
				; b5-3: ABIOS presence (011 = not supported)
				; b2: private
				; b1: memory split above 16Mb supported
				; b0: POSTEXT directly supported by POST
				;--------------------------------------------------------------------------
 F342  00						db      0
				;--------------------------------------------------------------------------
				; Feature byte 5 (IBM)
				; b1: enhanced mouse
				; b0: flash EPROM
				;--------------------------------------------------------------------------
 F343  00						db      0                                                


				; --------------------- INT 16h - keyboard interface ----------------
				;       AH      Description
				;       --      ------------------------------------------------
				;       00h     Get a key from the keyboard, return code in AX.
				;       01h     Test for available key, ZF=1 if none, ZF=0 and
				;               AX contains next key code if key available.
				;       02h     Get shift status. Returns shift key status in AL.
				;       03h     Set Autorepeat rate. BH=0,1,2,3 (delay time in quarter seconds), BL=0..1Fh for 30 char/sec to 2 char/sec repeat rate.
				;       05h     Store scan code (in CX) in the type ahead buffer.
				;       10h     Get a key (same as 00h in this implementation).
				;       11h     Test for key (same as 01h).
				;       12h     Get extended key status. Returns status in AX.

 = ds:[19h]			AltKpd          equ     <ds:[19h]>
 = ds:[1ah]			HeadPtr         equ     <ds:[1ah]>
 = ds:[1ch]			TailPtr         equ     <ds:[1ch]>
 = ds:[80h]			Buffer          equ     <ds:[80h]>;1eh
 = ds:[82h]			EndBuf          equ     <ds:[82h]>;3eh

 F344				int16 proc near
 F344  1E					push    ds
 F345  56					push    si
 F346  6A 40					push    40h
 F348  1F					pop     ds
 F349  86 C4					xchg    al, ah          ;shorter opcodes for al than ah
 F34B  48					dec     ax
 F34C  A8 EF					test    al, 0EFh        ;Check for 01h and 11h
 F34E  74 3E					jz      short TestKey   ;TestKey does not need cld
 F350  40					inc     ax
 F351  FC					cld
 F352  A8 EF					test    al, 0EFh        ;Check for 0h and 10h
 F354  74 1C					jz      short GetKey
 F356  3C 03					cmp     al, 3           ;Check for 02h and 03h
 F358  72 78					jb      short GetStatus
 F35A  74 7B					je      short SetAutoRpt   
 F35C  3C 05					cmp     al, 5           ;Check for StoreKey function.
 F35E  74 3D					je      short StoreKey
 F360  3C 09					cmp     al, 9           ;Get KB functionality
 F362  74 08					je      short kbfunc     
 F364  3C 12					cmp     al, 12h         ;Extended status call
 F366  74 56					je      short ExtStatus
 F368  3C 92					cmp     al, 92h         ;stupid keyb.com 
 F36A  75 02					jne     short Exit
 F36C				kbfunc:
 F36C  B0 24					mov     al, 24h         ;AL=20h (fn 10h, 12h supported, set typematic supported)        
 F36E				Exit:        
 F36E  5E					pop     si
 F36F  1F					pop     ds
 F370  CF					iret                    ; unknown function, Restores flags.

 F371				GetKey1:                        ; wait for interrupt
 F371  F4					hlt
 F372				GetKey: ; ----------- fn 00h, 10h
 F372  B4 11					mov     ah, 11h
 F374  CD 16					int     16h             ;See if key is available (IF becomes 1 after this int)
 F376  74 F9					jz      short GetKey1   ;Wait for keystroke.
 F378  FA					cli                     ;Critical region! Ints off.
 F379  8B 36 001A				mov     si, HeadPtr     ;Ptr to next character.
 F37D  AD					lodsw                   ;Get the character, Bump up HeadPtr
 F37E  3B 36 0082				cmp     si, EndBuf
 F382  72 04					jb      short noWrap
 F384  8B 36 0080				mov     si, Buffer
 F388				noWrap:             
 F388  89 36 001A				mov     HeadPtr, si
 F38C  EB E0					jmp     short Exit

 F38E				TestKey: ; ---------- fn 01h
 F38E  8B 36 001A				mov     si, HeadPtr
 F392  3B 36 001C				cmp     si, TailPtr     ;ZF=1, if empty buffer
 F396  AD					lodsw                   ;BIOS returns avail keycode.
 F397  FB					sti                     ;Ints back on.
 F398  5E					pop     si
 F399  1F					pop     ds
 F39A  CA 0002					retf    2               ;Pop flags (ZF is important!)

 F39D				StoreKey: ; ---------- fn 05h - Inserts the value in CX into the type ahead buffer.  
 F39D  8B 36 001C				mov     si, TailPtr     ;Address where we can put next key code.
 F3A1  89 0C					mov     [si], cx        ;Store the key code away
 F3A3  46					inc     si
 F3A4  46					inc     si              ;Move on to next entry in buf
 F3A5  3B 36 0082				cmp     si, EndBuf
 F3A9  72 04					jb      short NoWrap1
 F3AB  8B 36 0080				mov     si, Buffer
 F3AF				 NoWrap1:
 F3AF  B0 01					mov     al, 1           ;no room
 F3B1  3B 36 001A				cmp     si, HeadPtr     ;Data overrun?
 F3B5  74 B7					je      short Exit      ;if so, ignore key entry.
 F3B7  89 36 001C				mov     TailPtr, si
 F3BB  48					dec     ax              ;al=0
 F3BC  EB B0					jmp     short Exit       

 F3BE				ExtStatus: ; ------- fn 12h - Retrieve the extended keyboard status and return it in AH, and the standard keyboard status in AL.    
 F3BE  A0 0018					mov     al, KbdFlags2
 F3C1  24 77					and     al, 01110111b   ;Clear final sysreq field, and final right alt bit.
 F3C3  A8 04					test    al, 100b        ;Test cur sysreq bit.
 F3C5  74 02					jz      short NoSysReq  ;Skip if it's zero.
 F3C7  2C 84					sub     al, 10000100b   ;Set final sysreq bit, clear final right ctl bit.
 F3C9				NoSysReq:
 F3C9  8A 26 0096				mov     ah, KbdFlags3
 F3CD  80 E4 0C					and     ah, 1100b       ;Grab rt alt/ctrl bits.
 F3D0  0A E0					or      ah, al          ;Merge into AH.

 F3D2				GetStatus: ; --------- fn 02h     
 F3D2  A0 0017					mov     al, KbdFlags1   ;Just return Std Status.
 F3D5				Exit1:
 F3D5  EB 97					jmp     short Exit

 F3D7				SetAutoRpt: ; ------ fn 03h
 F3D7  80 FC 05					cmp     ah, 5
 F3DA  75 92					jne     short Exit
 F3DC  52					push    dx
 F3DD  C0 E7 05					shl     bh, 5
 F3E0  80 E3 1F					and     bl, 1fh
 F3E3  0A DF					or      bl, bh
 F3E5  80 E3 7F					and     bl, 7fh
 F3E8  B4 00					mov     ah, 0           ; wait LED update progress to finalize
 F3EA  E8 0028					call    WaitFlag        ; leaves with IF=0
 F3ED  72 23					jc      short timeout
 F3EF  80 0E 0097 08				or      byte ptr KbdFlags4, SetRepeat    ; set auto repeat in progress
 F3F4  B4 F3					mov     ah, 0f3h        ; set typematic rate and delay
 F3F6  53					push    bx
 F3F7  32 DB					xor     bl, bl          ; send to kb
 F3F9  E8 01AB					call    sendps2byte
 F3FC  5B					pop     bx
 F3FD  72 0E					jc      short timeout1  ; send timeout
 F3FF  B4 18					mov     ah, SetRepeat or AckReceived ; test if ACK received
 F401  E8 0011					call    WaitFlag
 F404  72 07					jc      short timeout1
 F406  8A E3					mov     ah, bl
 F408  32 DB					xor     bl, bl          ; send to kb
 F40A  E8 019A					call    sendps2byte     ; send data
 F40D				timeout1:
 F40D  80 26 0097 F7				and     byte ptr KbdFlags4, not SetRepeat   
 F412				timeout:
 F412  5A					pop     dx
 F413  EB C0					jmp     short Exit1


 F415				WaitFlag:   ; ah = desired KbdFlags4 & (AckReceived | LEDUpdate | SetRepeat)
 F415  BA 03DA					mov     dx, 3dah
 F418  B7 C8					mov     bh, 8*25    ; wait for max 25 * VGA frame time
 F41A				wf_loop:
 F41A  FA					cli
 F41B  A0 0097					mov     al, KbdFlags4
 F41E  24 58					and     al, AckReceived or LEDUpdate or SetRepeat
 F420  38 E0					cmp     al, ah
 F422  74 0A					je      short wf_ok ; flag ok, CF=0
 F424  FB					sti
 F425  EC					in      al, dx      ; get vblank
 F426  32 C7					xor     al, bh
 F428  24 08					and     al, 8h
 F42A  2A F8					sub     bh, al
 F42C  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F42E				wf_ok:
 F42E  C3					ret
 F42F				int16 endp

