 F42F  32 C7					xor     al, bh
 F431  24 08					and     al, 8h
 F433  2A F8					sub     bh, al
 F435  73 EC					jnc     short wf_loop     ; IBF - buffer full, no timeout
 F437				wf_ok:
 F437  C3					ret
 F438				int16 endp

